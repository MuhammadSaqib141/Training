docker tag <local-image> <dockerhub-username>/<repository-name>:<tag>


# Tag version 1.0
docker tag my-image-v1 yourusername/myapp:v1.0

# Tag version 2.0
docker tag my-image-v2 yourusername/myapp:v2.0

# Tag the latest version
docker tag my-image-latest yourusername/myapp:latest


docker push yourusername/myapp:v1.0
docker push yourusername/myapp:v2.0
docker push yourusername/myapp:latest

